this.sequenceForm1 = {}
this.editSequenceRowInfo = {}
this.editSequenceRowInfo = {
row:row,
index:index
}
this.editSequenceDialogVisible2 = true
var tempData = JSON.parse(JSON.stringify(row))
this.sequenceForm1 = tempData
},
